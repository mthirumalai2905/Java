import java.util.ArrayList;
import java.util.List;

public class MyProgram{
     public static void main(String[] args){
           List<String> myList = new ArrayList<String>();
           
           myList.add("apple");
           myList.add("banana");
           myList.add("orange");
           
           System.out.println(myList);
           
         }
         
   }
   

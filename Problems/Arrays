import java.util.Arrays;
public class Exercise77{
   public static void main(String[] args){
   int[] array1 = {50,60,89};
   int[] array2 = {20,-25,9};
   System.out.print("Arrays1: "+Arrays.toString(array1));
   System.out.print("Arrays2: "+Arrays.toString(array2));
   int array_new = {array1[0] + array2[2]};
   System.out.print("New Array: "+Arrays.toString(array_new));
   }
 }
